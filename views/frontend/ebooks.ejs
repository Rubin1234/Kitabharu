        </div>
        <div class="" style="line-height: 2;">
          <% ebookSubcategories.forEach(function(data){ %>
            <div class="form-check pl-0 mb-1">
              <input type="checkbox" class="form-check-input filled-in ebookcheckbox" id="<%= data.subcategory_name %>" subcategory_id="<%= data._id %>" onclick="ebookSubcategoryCheckbox()" <% if(data.slug == slug){%> <%= checked %> <% } %>>
              <label class="form-check-label small text-uppercase card-link-secondary" for="<%= data.subcategory_name %>"><%= data.subcategory_name %></label>
            </div>
          <% }) %>


        </div>

          
      </div>


